la valueOne = 5;
la valueTwo = 6;

la abs = fcn(a, b) {
    yedi(a > b){
        firta(a - b)
    } kita (a < b) {
        firta(b - a)
    } natra {
        firta(0)
    }
}

la result = abs(valueOne, valueTwo)
lekh(result)

